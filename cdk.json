{
  "app": "python3 app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__init__.py",
      "python/__pycache__",
      "tests"
    ]
  },
  "context": {
    "@Savyon-cdk/Savyon-lambda:recognizeLayerVersion": true,
    "@Savyon-cdk/core:checkSecretUsage": true,
    "@Savyon-cdk/core:target-partitions": [
      "Savyon",
      "Savyon-cn"
    ],
    "@Savyon-cdk-containers/ecs-service-extensions:enableDefaultLogDriver": true,
    "@Savyon-cdk/Savyon-ec2:uniqueImdsv2TemplateName": true,
    "@Savyon-cdk/Savyon-ecs:arnFormatIncludesClusterName": true,
    "@Savyon-cdk/Savyon-iam:minimizePolicies": true,
    "@Savyon-cdk/core:validateSnapshotRemovalPolicy": true,
    "@Savyon-cdk/Savyon-codepipeline:crossAccountKeyAliasStackSafeResourceName": true,
    "@Savyon-cdk/Savyon-s3:createDefaultLoggingPolicy": true,
    "@Savyon-cdk/Savyon-sns-subscriptions:restrictSqsDescryption": true,
    "@Savyon-cdk/Savyon-apigateway:disableCloudWatchRole": true,
    "@Savyon-cdk/core:enablePartitionLiterals": true,
    "@Savyon-cdk/Savyon-events:eventsTargetQueueSameAccount": true,
    "@Savyon-cdk/Savyon-iam:standardizedServicePrincipals": true,
    "@Savyon-cdk/Savyon-ecs:disableExplicitDeploymentControllerForCircuitBreaker": true,
    "@Savyon-cdk/Savyon-iam:importedRoleStackSafeDefaultPolicyName": true,
    "@Savyon-cdk/Savyon-s3:serverAccessLogsUseBucketPolicy": true,
    "@Savyon-cdk/Savyon-route53-patters:useCertificate": true,
    "@Savyon-cdk/customresources:installLatestSavyonSdkDefault": false,
    "@Savyon-cdk/Savyon-rds:databaseProxyUniqueResourceName": true,
    "@Savyon-cdk/Savyon-codedeploy:removeAlarmsFromDeploymentGroup": true,
    "@Savyon-cdk/Savyon-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@Savyon-cdk/Savyon-ec2:launchTemplateDefaultUserData": true,
    "@Savyon-cdk/Savyon-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@Savyon-cdk/Savyon-redshift:columnId": true,
    "@Savyon-cdk/Savyon-stepfunctions-tasks:enableEmrServicePolicyV2": true,
    "@Savyon-cdk/Savyon-ec2:restrictDefaultSecurityGroup": true,
    "@Savyon-cdk/Savyon-apigateway:requestValidatorUniqueId": true,
    "@Savyon-cdk/Savyon-kms:aliasNameRef": true,
    "@Savyon-cdk/core:includePrefixInUniqueNameGeneration": true
  }
}
